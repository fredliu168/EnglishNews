{
  "extends": "eslint-config-google",
  "plugins": [
    "html"
  ],
  "rules": {
    "indent": [2, 4],
    "semi": [2, 'always'],
    "no-negated-condition": 0,
    "eqeqeq": [2, 'allow-null'],
    "no-eq-null": 0,
    "quote-props": [2, 'as-needed'],
    "no-new": 0,
    "max-len": ["error", 120]
  }
}
